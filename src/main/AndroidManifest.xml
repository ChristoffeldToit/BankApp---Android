<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.sisonkebankapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/t"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".test"></activity>
        <activity android:name=".main_page" />
        <activity
            android:name=".DatabaseHelper"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".transferFunds"
            android:parentActivityName=".main_page" />
        <activity
            android:name=".viewAccountBalance"
            android:parentActivityName=".main_page" />
        <activity android:name=".loginActivity" />
        <activity android:name=".registration_activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>